<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>PercentMatch__c</fullName>
    <formula>IF(
CONTAINS( TEXT(Type__c) , &quot;OAA&quot;),
 Gross_Amount__c * 0.1,
0
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>For certain Funding Sources, this % is removed from the reimbursement calculation.</inlineHelpText>
    <label>10% Match</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>
